import random

nums = [0] * 10

for i in range(len(nums)):
    nums[i] = random.randint(1, 100)

print(nums)

for i in range(len(nums) - 1):
    min_idx = i
    for j in range(i + 1, len(nums)):
        if nums[min_idx] > nums[j]:
            min_idx = j

    temp = nums[min_idx]
    nums[min_idx] = nums[i]
    nums[i] = temp

    print(nums)
